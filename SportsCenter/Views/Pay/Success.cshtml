<div class="back_re">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="title">
                    <h2>訂單成功</h2>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="success">
    <div class="tab-pane" id="tabthree" role="tabpanel" aria-labelledby="tabthree-tab" tabindex="0">
        <div class="container tabthree">
            <div class="container">
                <table class="table table-hover">
                    <thead>
                        <tr>
                            <th scope="col">商品名稱</th>
                            <th scope="col">商品數量</th>
                            <th scope="col">商品價格</th>
                            <th scope="col">商品總價</th>
                            <th scope="col">下單日期</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr v-for="item in list">
                            <td>{{item.name}}</td>
                            <td>{{item.count}}</td>
                            <td>{{item.price}}</td>
                            <td>{{item.total}}</td>
                            <td>{{item.time}}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

<script>
    var app = new Vue({
        el: "#success",
        data: {
            list:[]
        },
        mounted: function () {
            let self = this;
            axios.get('/api/Product/GetSuccess').then(result => {
                self.list = result.data;
            });
        }
    })
</script>